/* pages/my.wxss */

/* bottomCoupon */

.bottomCoupon {
    position: absolute;
    bottom: 0;
    width: 750rpx;
    height: 633rpx;
    background: rgba(245, 245, 245, 1);
}

.bottomCoupon .title1 {
    box-sizing: border-box;
    padding: 0 40rpx;
    width: 750rpx;
    height: 88rpx;
    background: rgba(255, 255, 255, 1);
    font-size: 28rpx;
    font-family: MicrosoftYaHei;
    font-weight: 400;
    color: rgba(51, 51, 51, 1);
    line-height: 88rpx;
    text-align: center;
}

.bottomCoupon image {
    position: absolute;
    right: 40rpx;
    top: 28rpx;
}

.ticket {
    position: relative;
    margin: 20rpx 30rpx;
    width: 690rpx;
    height: 234rpx;
    background: rgba(255, 255, 255, 1);
    border-radius: 10rpx;
    overflow: hidden;
}

.ticket .get {
    position: absolute;
    left: 538rpx;
    top: 40rpx;
    width: 121rpx;
    height: 45rpx;
    background: rgba(255, 111, 61, 1);
    border-radius: 23rpx;
    font-size: 26rpx;
    font-family: MicrosoftYaHei;
    font-weight: 400;
    color: rgba(255, 255, 255, 1);
    line-height: 45rpx;
    text-align: center;
}

.upBox {
    height: 171rpx;
    width: 100%;
    box-sizing: border-box;
    padding: 30rpx 0 32rpx 29rpx;
    border-bottom: 1px dashed #e0e0e0;
}

.bottomBox {
    width: 100%;
    box-sizing: border-box;
    padding: 20rpx 0 19rpx 30rpx;
}

.bottomBox .text {
    height: 23rpx;
    font-size: 24rpx;
    font-family: SourceHanSansCN-Regular;
    font-weight: 400;
    color: rgba(153, 153, 153, 1);
    line-height: 23rpx;
    text-align: left;
}

/* 优惠券主要结构 */

.contentBox {
    position: absolute;
    left: 240rpx;
    top: 30rpx;
    width: 260rpx;
    height: 106rpx;
}

.contentBox .title {
    height: 28rpx;
    font-size: 30rpx;
    font-family: SourceHanSansCN-Regular;
    font-weight: 400;
    color: rgba(26, 26, 26, 1);
    line-height: 28rpx;
}

.contentBox .normal {
    height: 25rpx;
    font-size: 26rpx;
    font-family: SourceHanSansCN-Regular;
    font-weight: 400;
    color: rgba(153, 153, 153, 1);
    line-height: 25rpx;
}

.contentBox .view {
    text-align: left;
}

/* priceBox */

.priceBox {
    position: absolute;
    left: 29rpx;
    top: 31rpx;
    width: 144rpx;
    height: 108rpx;
}

.bigPrice {
    width: 100%;
    height: 62rpx;
}

.bigPrice .symbol {
    display: inline;
    font-size: 42rpx;
    font-family: SourceHanSansCN-Bold;
    font-weight: bold;
    color: rgba(255, 93, 22, 1);
    line-height: 42rpx;
}

.bigPrice .price {
    display: inline;
    font-size: 80rpx;
    font-family: SourceHanSansCN-Bold;
    font-weight: bold;
    color: rgba(255, 93, 22, 1);
    height: 62rpx;
    line-height: 62rpx;
}

.condition {
    height: 26rpx;
    font-size: 28rpx;
    font-family: SourceHanSansCN-Bold;
    font-weight: bold;
    color: rgba(255, 93, 22, 1);
    line-height: 26rpx;
}

/* 优惠券结束 */

.header {
    width: 750rpx;
    height: 85rpx;
    padding: 0 30rpx;
    position: absolute;
    top: 0;
    left: 0;
    z-index: 10;
}

.avator {
    position: absolute;
    left: 30rpx;
    width: 78rpx;
}

.location {
    width: 138rpx;
    font-size: 28rpx;
    font-family: SourceHanSansCN-Regular;
    font-weight: 400;
    color: rgba(26, 26, 26, 1);
    line-height: 55rpx;
}

.location_arrow {
    position: absolute;
    right: 0;
    top: 24rpx;
    width: 20rpx;
}

.inputText {
    position: absolute;
    bottom: 13rpx;
    left: 346rpx;
    float: right;
    width: 374rpx;
    height: 60rpx;
    background: rgba(245, 245, 245, 1);
    border-radius: 30rpx;
    padding-left: 70rpx;
    font-size: 26rpx;
}

.input_searchIcon {
    position: absolute;
    top: 27rpx;
    left: 371rpx;
    width: 30rpx;
    height: 30rpx;
}

.place_input {
    width: 52rpx;
    height: 26rpx;
    font-size: 26rpx;
    font-family: MicrosoftYaHei;
    font-weight: 400;
    color: rgba(204, 204, 204, 1);
}

/* scrollShopList */

.scrollShopList {
    height: 100vh;
    padding-top: 85rpx;
    box-sizing: border-box;
    /* height: 1023rpx; */
}

.mainMenu {
    float: left;
    width: 184rpx;
    height: 100%;
    background: #f5f5f5;
}

.rightBox {
    float: left;
    position: relative;
    height: 100%;
    width: 566rpx;
}

.mainMenu .item {
    width: 184rpx;
    height: 105rpx;
    text-align: center;
    line-height: 105rpx;
}

.mainMenu .item .text {
    font-size: 28rpx;
    font-family: MicrosoftYaHei;
    font-weight: 400;
    color: rgba(102, 102, 102, 1);
    line-height: 105rpx;
}

.mainMenu .item_act {
    position: relative;
    background: rgba(255, 255, 255, 1);
}

.mainMenu .item_act ::before {
    position: absolute;
    left: 0;
    top: 41rpx;
    content: '';
    display: block;
    width: 6rpx;
    height: 23rpx;
    background: rgba(255, 126, 76, 1);
}

.mainMenu .item_act .text {
    font-size: 28rpx;
    font-family: MicrosoftYaHei-Bold;
    font-weight: bold;
    color: rgba(51, 51, 51, 1);
}

.subMenu {
    position: absolute;
    top: 0;
    left: 0;
    height: 93rpx;
    border-bottom: 1px solid #e5e5e5;
}

.subMenu .item {
    display: inline-block;
    margin-left: 30rpx;
}

.subMenu .item .text {
    font-size: 26rpx;
    font-family: MicrosoftYaHei;
    font-weight: 400;
    color: rgba(51, 51, 51, 1);
    line-height: 93rpx;
}

.subMenu .item_act .text {
    color: rgba(255, 126, 76, 1);
}

/* goods_item */

.scroll_a, .scroll_b {
    height: 100%;
}

.scroll_b {
}

.shoppingList {
    padding-top: 93rpx;
    height: 100%;
    box-sizing: border-box;
}

.goods_item {
    height: 248rpx;
    width: 566rpx;
    border-bottom: 1px solid #e5e5e5;
    box-sizing: border-box;
    padding: 44rpx 30rpx 47rpx 7rpx;
}

.goods_item:last-child {
    border: none;
}

.goods_item .img {
    width: 188rpx;
}

.content {
    position: relative;
    width: 331rpx;
    height: 100%;
    box-sizing: border-box;
    padding-top: 7rpx;
}

.flexStyle {
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}

.content .title {
    height: 28rpx;
    font-size: 28rpx;
    font-family: MicrosoftYaHei;
    font-weight: 400;
    color: rgba(26, 26, 26, 1);
    line-height: 28rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.content .introduction {
    font-size: 24rpx;
    font-family: MicrosoftYaHei;
    font-weight: 400;
    color: rgba(153, 153, 153, 1);
    line-height: 24rpx;
    margin-top: 15rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.content .price {
    font-size: 28rpx;
    font-family: MicrosoftYaHei;
    font-weight: 400;
    color: rgba(26, 26, 26, 1);
    line-height: 28rpx;
    margin-top: 25rpx;
}

.content .offerPrice {
    height: 22rpx;
    font-size: 28rpx;
    font-family: MicrosoftYaHei-Bold;
    font-weight: bold;
    color: rgba(88, 181, 48, 1);
    line-height: 22rpx;
}

.content .youhuiBox {
    width: 79rpx;
    position: relative;
    height: 29rpx;
}

.content .shopping {
    position: absolute;
    right: 0;
    bottom: 0;
    width: 60rpx;
    background: rgba(240, 255, 234, 1);
    border-radius: 50%;
}

.youhuiText {
    position: absolute;
    left: 12rpx;
    top: 0;
    width: 56rpx;
    height: 15rpx;
    line-height: 29rpx;
    font-size: 16rpx;
    font-family: SourceHanSansCN-Regular;
    font-weight: 400;
    color: rgba(88, 181, 48, 1);
    z-index: 10;
}

