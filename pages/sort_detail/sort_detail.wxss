.imgText {
    box-sizing: border-box;
    padding: 0 30rpx 47rpx;
}

.foodsImg {
    width: 690rpx;
    height: 365rpx;
}

.swiper_item {
    width: 690rpx;
    height: 365rpx;
}

.swiper_item_img {
    width: 690rpx;
    height: 365rpx;
}

.contentBox {
    position: relative;
    height: 262rpx;
    padding-top: 36rpx;
    box-sizing: border-box;
}

.flexStyle {
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}

.contentBox .title {
    height: 28rpx;
    font-size: 28rpx;
    font-family: MicrosoftYaHei;
    font-weight: 400;
    color: rgba(51, 51, 51, 1);
    line-height: 28rpx;
}

.contentBox .introduction {
    position: absolute;
    top: 83rpx;
    height: 26rpx;
    font-size: 26rpx;
    font-family: MicrosoftYaHei;
    font-weight: 400;
    color: rgba(102, 102, 102, 1);
    line-height: 26rpx;
}

.contentBox .price {
    position: absolute;
    top: 135rpx;
    height: 22rpx;
    font-size: 28rpx;
    font-family: MicrosoftYaHei;
    font-weight: 400;
    color: rgba(26, 26, 26, 1);
    line-height: 22rpx;
}

.contentBox .offerPrice {
    position: absolute;
    top: 184rpx;
    height: 27rpx;
    font-size: 34rpx;
    font-family: MicrosoftYaHei-Bold;
    font-weight: bold;
    color: rgba(88, 181, 48, 1);
    line-height: 27rpx;
}

.contentBox .youhuiBox {
    position: relative;
    margin-top: -4rpx;
    width: 79rpx;
    height: 29rpx;
}

.youhuiText {
    position: absolute;
    left: 12rpx;
    top: 0;
    height: 15rpx;
    font-size: 16rpx;
    font-family: SourceHanSansCN-Regular;
    font-weight: 400;
    color: rgba(88, 181, 48, 1);
    line-height: 29rpx;
}

.bottomText {
    height: 26rpx;
    font-size: 26rpx;
    font-family: MicrosoftYaHei;
    font-weight: 400;
    color: rgba(102, 102, 102, 1);
    line-height: 26rpx;
}

.sellCount {
    position: absolute;
    left: 585rpx;
    top: 553rpx;
    height: 24rpx;
    font-size: 24rpx;
    font-family: MicrosoftYaHei;
    font-weight: 400;
    color: rgba(102, 102, 102, 1);
    line-height: 18rpx;
}

/* 下部分的详情 */

.bottomDetail {
    width: 750rpx;
}

.bottomDetail .title {
    height: 103rpx;
    border-bottom: 1px solid rgba(227, 228, 228, 1);
    text-align: center;
}

.bottomDetail .title .text {
    height: 26rpx;
    font-size: 26rpx;
    font-family: MicrosoftYaHei;
    font-weight: 400;
    color: rgba(102, 102, 102, 1);
    line-height: 103rpx;
}

.showDetail {
    padding: 0 5rpx;
}

.showDetail .name {
    height: 26rpx;
    font-size: 26rpx;
    font-family: MicrosoftYaHei;
    font-weight: 400;
    color: rgba(102, 102, 102, 1);
}

.showDetail .data {
    height: 26rpx;
    font-size: 26rpx;
    font-family: MicrosoftYaHei;
    font-weight: 400;
    color: rgba(153, 153, 153, 1);
    padding-left: 15rpx;
}

.render {
    /* padding: 26rpx 30rpx 30rpx; */
    padding: 26rpx 0 30rpx;
}

.renderImg {
    width: 690rpx;
    height: 510rpx;
    background: pink;
}

.detail1 {
    height: 64rpx;
    border-bottom: 1px solid rgba(227, 228, 228, 1);
    line-height: 64rpx;
}

.detail2 {
    height: 67rpx;
    line-height: 67rpx;
    border-bottom: 1px solid rgba(243, 243, 243, 1);
}

.detail3 {
    height: 80rpx;
    line-height: 80rpx;
    border-bottom: 1px solid rgba(243, 243, 243, 1);
}

.detail1, .detail2, .detail3 {
    padding: 0 27rpx;
}

/* 购物车 */

.cart {
    position: fixed;
    display: flex;
    left: 0;
    bottom: 0;
    width: 750rpx;
    height: 95rpx;
    background: rgba(255, 255, 255, 1);
}

.icon {
    flex-shrink: 0;
    display: flex;
    justify-content: center;
    align-items: center;
    align-content: center;
    width: 203rpx;
    height: 95rpx;
    background: #fff;
}

.icon view {
    padding: 5rpx 8rpx;
    border-radius: 10rpx;
    font-size: 18rpx;
    background-color: #58b530;
    text-align: center;
    color: #fff;
    position: absolute;
    top: 12rpx;
    left: 120rpx;
}

.icon image {
    display: block;
    width: 53rpx;
    height: 44rpx;
}

.gotoCart {
    flex-grow: 1;
    height: 95rpx;
    background: rgba(88, 181, 48, 1);
    box-shadow: 0px 0px 24px 0px rgba(127, 130, 125, 0.26);
    text-align: center;
}

.gotoCart_nodata {
    background: rgba(204, 204, 204, 1);
    box-shadow: 0px 0px 24rpx 0px rgba(127, 130, 125, 0.26);
}

.gotoCart .text {
    height: 30rpx;
    font-size: 30rpx;
    font-family: MicrosoftYaHei;
    font-weight: 400;
    color: rgba(255, 255, 255, 1);
    line-height: 95rpx;
}

.de_flexStyle {
    display: flex;
}
